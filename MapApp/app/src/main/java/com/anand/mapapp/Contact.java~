package com.app2.aar.qr_code;

/**
 * Created by user on 22/6/15.
 */
public class Contact {

    //private variables
    int _id;
    String _link;
    String _x;
    String _y;

    // Empty constructor
    public Contact(){

    }
    // constructor
    public Contact(int id, String link, String x, String y){
        this._id = id;
        this._link = link;
        this._x = x;
        this._y = y;
    }

    // constructor
    public Contact(String link, String _x, String _y){
        this._link = link;
        this._x = _x;
        this._y = _y;
    }
    // getting ID
    public int getID(){
        return this._id;
    }

    // setting id
    public void setID(int id){
        this._id = id;
    }

    // getting name
    public String getLink(){
        return this._link;
    }

    // setting name
    public void setLink(String link){
        this._link = link;
    }

    // getting phone number
    public String getX(){
        return this._x;
    }
    public String getY(){
        return this._y;
    }

    // setting phone number
    public void setX(String x){
        this._x = x;
    }

    public void setY(String y){
        this._y = y;
    }
}

